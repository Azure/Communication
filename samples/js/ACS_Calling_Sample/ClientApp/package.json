{
  "name": "acs_calling_sample",
  "version": "0.3.0",
  "private": true,
  "description": "This is a sample application to show how the ACS Calling Web SDK can be used to build a group calling experience. The client-side application is a React based user interface which uses Redux for handling complex state while leveraging Microsoft Fluent UI for how things look. Powering this front-end is a C# web application powered by ASP.net Core to connect this application with Azure.",
  "dependencies": {
    "@azure/communication-common": "^1.0.0-dev.20200708.5",
    "@fluentui/react": "^7.117.1",
    "@fluentui/react-icons-northstar": "0.50.0",
    "@fluentui/react-northstar": "0.50.0",
    "@skype/spool-sdk": "1.1.54",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "uuid": "^8.1.0"
  },
  "scripts": {
    "start": "webpack-dev-server --port 8081 --mode=development",
    "setup-vsts-auth": "npm i -g better-vsts-npm-auth --registry https://registry.npmjs.com && better-vsts-npm-auth",
    "build:dev": "webpack --progress --mode=development",
    "build:prod": "cross-env NODE_ENV=production webpack --progress --mode=production",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json",
    "prettier": "prettier --write --config .prettierrc **/*.js **/*.ts",
    "prettier:check": "prettier --check --config .prettierrc **/*.js **/*.ts",
    "lint": "eslint */**/*.{ts,tsx} && echo Lint completed.",
    "lint:quiet": "eslint */**/*.{ts,tsx} --quiet && echo Lint completed."
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime": "^7.8.7",
    "@types/node": "^12.12.6",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.8.0",
    "@types/react-redux": "^7.1.9",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-standard": "^4.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "2.0.5",
    "style-loader": "^1.1.3",
    "ts-loader": "^7.0.4",
    "typescript": "^3.9.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "compression-webpack-plugin": "^4.0.0",
    "webpack-dev-server": "^3.10.3"
  }
}
